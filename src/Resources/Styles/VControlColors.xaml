<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary x:Class="VControl.Styles.VControlColors"
                    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--MD3静态标准色Light-->

    <Color x:Key="Primary">#FF7415</Color>
    <Color x:Key="OnPrimary">#FFFFFF</Color>
    <Color x:Key="PrimaryContainer">#FEF4EA</Color>
    <Color x:Key="OnPrimaryContainer">#ED6103</Color>

    <Color x:Key="Secondary">#f38e47</Color>
    <Color x:Key="OnSecondary">#FFFFFF</Color>
    <Color x:Key="SecondaryContainer">#FEF4EA</Color>
    <Color x:Key="OnSecondaryContainer">#EB6B13</Color>

    <Color x:Key="Tertiary">#FDDB3A</Color>
    <Color x:Key="OnTertiary">#2C3639</Color>
    <Color x:Key="TertiaryContainer">#786301</Color>
    <Color x:Key="OnTertiaryContainer">#fef1b0</Color>

    <Color x:Key="Surface">#FFFFFF</Color>
    <Color x:Key="OnSurface">#3F4E4F</Color>
    <Color x:Key="SurfaceVariant">#E7E0EC</Color>
    <Color x:Key="OnSurfaceVariant">#49454E</Color>

    <Color x:Key="Background">#F5F5F5</Color>
    <Color x:Key="OnBackground">#1C1B1F</Color>

    <Color x:Key="Outline">#E1E1E1</Color>
    <Color x:Key="OutlineVariant">#C4C7C5</Color>
    <Color x:Key="Shadow">#000000</Color>






    <!--MD3静态标准色Dark-->
    <Color x:Key="PrimaryDark">#a8eac0</Color>
    <Color x:Key="OnPrimaryDark">#00553b</Color>
    <Color x:Key="PrimaryContainerDark">#001b11</Color>
    <Color x:Key="OnPrimaryContainerDark">#baefcd</Color>

    <Color x:Key="SecondaryDark">#daffbe</Color>
    <Color x:Key="OnSecondaryDark">#52be00</Color>
    <Color x:Key="SecondaryContainerDark">#007a57</Color>
    <Color x:Key="OnSecondaryContainerDark">#e1ffcb</Color>

    <Color x:Key="TertiaryDark">#feed9d</Color>
    <Color x:Key="OnTertiaryDark">#9a7f02</Color>
    <Color x:Key="TertiaryContainerDark">#8b7301</Color>
    <Color x:Key="OnTertiaryContainerDark">#fef1b0</Color>

    <Color x:Key="SurfaceDark">#2C3639</Color>
    <Color x:Key="OnSurfaceDark">#F6F4E6</Color>
    <Color x:Key="SurfaceVariantDark">#49454F</Color>
    <Color x:Key="OnSurfaceVariantDark">#DCD7C9</Color>

    <Color x:Key="BackgroundDark">#1C1B1F</Color>
    <Color x:Key="OnBackgroundDark">#E6E1E5</Color>

    <Color x:Key="OutlineDark">#938F99</Color>
    <Color x:Key="OutlineVariantDark">#444746</Color>
    <Color x:Key="ShadowDark">#000000</Color>

    <!--MD3其他颜色-->


    <Color x:Key="Error">#B3261E</Color>
    <Color x:Key="ErrorContainer">#F9DEDC</Color>

    <Color x:Key="Success">#01ae54</Color>
    <Color x:Key="SuccessContainer">#a8eac0</Color>

    <Color x:Key="Warning">#F7B548</Color>
    <Color x:Key="WarningContainer">#FFE5B9</Color>

    <Color x:Key="DisabledText">#ba919191</Color>
    <Color x:Key="PlaceHolderText">#E0E0E0</Color>
    <Color x:Key="DisabledBackground">#80919191</Color>


    <!--BRUSHES-->
    <LinearGradientBrush x:Key="PrimaryBrush">
        <GradientStop Offset="0"
                      Color="{StaticResource Secondary}" />
        <GradientStop Offset="1"
                      Color="{StaticResource Primary}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PrimaryContainerDarkBrush">
        <GradientStop Offset="0"
                      Color="{StaticResource PrimaryContainerDark}" />
        <GradientStop Offset="1"
                      Color="{StaticResource SecondaryContainerDark}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PrimaryContainerBrush">
        <GradientStop Offset="0"
                      Color="{StaticResource Secondary}" />
        <GradientStop Offset="1"
                      Color="{StaticResource Primary}" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="SecondaryBrush"
                     Color="{StaticResource Secondary}" />
    <SolidColorBrush x:Key="OnSecondaryContainerBrush"
                     Color="{StaticResource OnSecondaryContainer}" />

    <SolidColorBrush x:Key="TertiaryBrush"
                     Color="{StaticResource Tertiary}" />
    <SolidColorBrush x:Key="TertiaryContainerBrush"
                     Color="{StaticResource TertiaryContainer}" />
    <SolidColorBrush x:Key="SurfaceBrush"
                     Color="{StaticResource Surface}" />
    <SolidColorBrush x:Key="SurfaceVariantBrush"
                     Color="{StaticResource SurfaceVariant}" />

    <SolidColorBrush x:Key="BackgroundBrush"
                     Color="{StaticResource Background}" />


    <SolidColorBrush x:Key="OnPrimaryBrush"
                     Color="{StaticResource OnPrimary}" />
    <SolidColorBrush x:Key="OnPrimaryContainerBrush"
                     Color="{StaticResource OnPrimaryContainer}" />
    <SolidColorBrush x:Key="OnTertiaryBrush"
                     Color="{StaticResource OnTertiary}" />
    <SolidColorBrush x:Key="OnTertiaryContainerBrush"
                     Color="{StaticResource OnTertiaryContainer}" />

    <SolidColorBrush x:Key="OnSurfaceBrush"
                     Color="{StaticResource OnSurface}" />
    <SolidColorBrush x:Key="OnSurfaceVariantBrush"
                     Color="{StaticResource OnSurfaceVariant}" />


    <SolidColorBrush x:Key="OnBackgroundBrush"
                     Color="{StaticResource OnBackground}" />

    <SolidColorBrush x:Key="OutlineBrush"
                     Color="{StaticResource Outline}" />
    <SolidColorBrush x:Key="OutlineVariantBrush"
                     Color="{StaticResource OutlineVariant}" />

    <SolidColorBrush x:Key="ShadowBrush"
                     Color="{StaticResource Shadow}" />

    <!--自定义颜色-->

    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>

    <Color x:Key="Gray50">#F8F8F8</Color>
    <Color x:Key="Gray60">#F7F7F7</Color>
    <Color x:Key="Gray100">#F5F5F5</Color>
    <Color x:Key="Gray120">#F3F3F3</Color>
    <Color x:Key="Gray150">#F1F1F1</Color>
    <Color x:Key="Gray170">#F0F0F0</Color>
    <Color x:Key="Gray190">#E1E1E1</Color>
    <Color x:Key="Gray200">#E0E0E0</Color>
    <Color x:Key="Gray230">#B7B7B7</Color>
    <Color x:Key="Gray240">#B5B5B5</Color>
    <Color x:Key="Gray250">#B3B3B3</Color>
    <Color x:Key="Gray270">#AEAEAE</Color>
    <Color x:Key="Gray300">#ACACAC</Color>
    <Color x:Key="Gray360">#A6A6A6</Color>
    <Color x:Key="Gray380">#919191</Color>
    <Color x:Key="Gray400">#848381</Color>
    <Color x:Key="Gray500">#6E6E6E</Color>
    <Color x:Key="Gray550">#666666</Color>
    <Color x:Key="Gray600">#404040</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#141414</Color>

    <SolidColorBrush x:Key="WhiteBrush"
                     Color="{StaticResource White}" />
    <SolidColorBrush x:Key="BlackBrush"
                     Color="{StaticResource Black}" />
    <SolidColorBrush x:Key="Gray100Brush"
                     Color="{StaticResource Gray100}" />
    <SolidColorBrush x:Key="Gray200Brush"
                     Color="{StaticResource Gray200}" />
    <SolidColorBrush x:Key="Gray300Brush"
                     Color="{StaticResource Gray300}" />
    <SolidColorBrush x:Key="Gray400Brush"
                     Color="{StaticResource Gray400}" />
    <SolidColorBrush x:Key="Gray500Brush"
                     Color="{StaticResource Gray500}" />
    <SolidColorBrush x:Key="Gray600Brush"
                     Color="{StaticResource Gray600}" />
    <SolidColorBrush x:Key="Gray900Brush"
                     Color="{StaticResource Gray900}" />
    <SolidColorBrush x:Key="Gray950Brush"
                     Color="{StaticResource Gray950}" />


</ResourceDictionary>